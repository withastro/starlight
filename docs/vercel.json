{
  "$schema": "https://openapi.vercel.sh/vercel.json",

  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/zh/(.*)",
      "dest": "/zh-cn/$1",
      "status": 301
    },
    {
      "src": "/ph/(.*)",
      "dest": "https://astro-houston-ph.pages.dev/ph/$1",
      "status": 200
    },
    {
      "src": "/(?<lang>[^/]*)/(.*)",
      "dest": "/$lang/404",
      "status": 404
    }
  ]
}
